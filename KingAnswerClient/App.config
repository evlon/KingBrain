<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--代理服务器-->
    <add key="proxyServer" value="ws://localhost:5534"/>

    <!--是否自动继续游戏-->
    <add key="autoContinue" value="true"/>

    <!--adb工具包路径-->
    <add key="adb" value="D:\platform-tools-latest-windows\platform-tools\adb.exe"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
